{
 "name": "GNIB/IRP appointment scanner",
  "version": "1.0",
  "description": "Tired of refreshing INIS page every second to book an appointment? With this extension you can just setup your information and let the app do the hard work. Once an appoinment is found, a sound will play to alert so you can book your appointment.",
  "permissions": ["activeTab", "declarativeContent", "storage", "notifications"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js", "gnib.js", "jquery-3.4.1.min.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
